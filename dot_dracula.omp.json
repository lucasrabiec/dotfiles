{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#282A36",
    "comment": "#6272A4",
    "current-line": "#44475A",
    "cyan": "#8BE9FD",
    "foreground": "#F8F8F2",
    "green": "#50FA7B",
    "orange": "#FFB86C",
    "pink": "#FF79C6",
    "purple": "#BD93F9",
    "red": "#FF5555",
    "yellow": "#F1FA8C"
  },
  "console_title_template": "{{ .Folder }}",
  "terminal_background": "p:background",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "p:foreground",
          "background": "p:comment",
          "type": "os",
          "style": "diamond"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uf07c {{ .Path }} ",
          "foreground": "p:background",
          "background": "p:purple",
          "options": {
            "style": "agnoster_short",
            "max_depth": 2
          }
        },
        {
          "options": {
            "branch_icon": " \uf126 ",
            "tag_icon": " \uf412 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if or (.Working.Changed) (.Staging.Changed) }}{{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:orange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:cyan{{ end }}",
            "{{ if gt .Behind 0 }}p:red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "always_enabled": true
          },
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "foreground": "p:green",
          "powerline_symbol": "\ue0b2",
          "background": "#000000",
          "type": "status",
          "style": "powerline",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ],
          "invert_powerline": true
        },
        {
          "template": " {{ .FormattedMs }} \uf252 ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:orange",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " \uf0ad ",
          "foreground": "p:red",
          "powerline_symbol": "\ue0b2",
          "background": "p:background",
          "type": "root",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "files"
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue798 ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:cyan",
          "type": "dart",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " {{ if .Unsupported }}\uf071 (unsupported){{ else }}{{ .Full }}{{ end }} \ue77f ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:purple",
          "type": "dotnet",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:cyan",
          "type": "go",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue81b ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:purple",
          "type": "kotlin",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_version": true,
            "fetch_package_manager": true
          },
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ if .PackageManagerName }}({{ .PackageManagerName }}) {{end }}{{ .Full }} \ue718 ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:green",
          "type": "node",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_version": true,
            "fetch_virtual_env": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} \ue235 ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:cyan",
          "type": "python",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:red",
          "type": "ruby",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue68b ",
          "foreground": "p:background",
          "powerline_symbol": "\ue0b2",
          "background": "p:orange",
          "type": "rust",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "display_default": false
          },
          "template": " {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} \ue7ad ",
          "powerline_symbol": "\ue0b2",
          "background": "p:orange",
          "type": "aws",
          "style": "powerline",
          "foreground_templates": [
            "{{if contains \"default\" .Profile}}p:foreground{{end}}",
            "{{if contains \"jan\" .Profile}}p:background{{end}}"
          ],
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue7dd ",
          "powerline_symbol": "\ue0b2",
          "foreground": "p:cyan",
          "background": "p:background",
          "type": "flutter",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue7ba ",
          "powerline_symbol": "\ue0b2",
          "foreground": "p:cyan",
          "background": "p:background",
          "type": "react",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "p:foreground",
          "background": "p:comment",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "\u276f",
          "foreground": "p:green",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ],
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 4,
  "final_space": true
}
